<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import org.tbee.javafx.scene.layout.fxml.MigPane?>

<MigPane id="rootPane" fx:controller="uk.ac.ebi.pride.toolsuite.prideadvisor.controller.AdvisorController"
        styleClass="main-panel"
        layout="insets 20"
        cols="[label, pref!][grow, 50::]"
        rows=""
        xmlns:fx="http://javafx.com/fxml">

    <Button fx:id="setupValidate" text="Validate a file" onAction="#setScreenValidate" MigPane.cc="skip, gap :push, gaptop 15, wrap" />
    <Button fx:id="setupConvert" text="Convert a file" onAction="#setScreenConvert" MigPane.cc="skip, gap :push, gaptop 15, wrap" />


</MigPane >